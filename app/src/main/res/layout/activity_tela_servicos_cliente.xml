<?xml version="1.0" encoding="utf-8"?>

<androidx.core.widget.NestedScrollView xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/background">


        <EditText
            android:textColorHint="@color/white"
            android:id="@+id/pesquisa"
            android:layout_width="258dp"
            android:layout_height="48dp"
            android:layout_marginTop="36dp"
            android:drawableRight="@drawable/ic_search"
            android:ems="10"
            android:hint="Pesquisar serviÃ§os"
            android:inputType="textPersonName"
            android:textSize="21dp"

            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />



        <Button
            android:id="@+id/bt_deslogar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="20dp"
            android:layout_marginBottom="52dp"
            android:backgroundTint="@color/black"
            android:padding="20dp"
            android:text="Deslogar"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.4"
            app:layout_constraintStart_toStartOf="parent" />


        <View
            android:id="@+id/containerComponents"
            style="@style/ContainerComponents"
            android:layout_width="362dp"
            android:layout_height="503dp"
            android:layout_marginBottom="16dp"
            app:layout_constraintBottom_toTopOf="@+id/bt_deslogar"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.489"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/pesquisa"
            app:layout_constraintVertical_bias="0.937" />

        <EditText
            android:id="@+id/historico1"
            style="@style/Edit_Text"
            android:layout_width="323dp"
            android:layout_height="68dp"
            android:layout_marginTop="84dp"
            android:drawableLeft="@drawable/ic_positivo"
            android:drawableRight="@mipmap/ic_star"
            android:inputType="numberPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/containerComponents" />

        <EditText
            android:id="@+id/historico2"
            style="@style/Edit_Text"
            android:layout_width="323dp"
            android:layout_height="68dp"
            android:layout_marginTop="16dp"
            android:drawableLeft="@drawable/ic_negativo"
            android:drawableRight="@drawable/ic_star"
            android:inputType="numberPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/historico1" />

        <EditText
            android:id="@+id/historico3"
            style="@style/Edit_Text"
            android:layout_width="323dp"
            android:layout_height="68dp"
            android:layout_marginTop="16dp"
            android:drawableLeft="@drawable/ic_positivo"
            android:drawableRight="@mipmap/ic_star"
            android:inputType="numberPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/historico2" />

        <EditText
            android:id="@+id/historico4"
            style="@style/Edit_Text"
            android:layout_width="323dp"
            android:layout_height="68dp"
            android:layout_marginTop="16dp"
            android:drawableLeft="@drawable/ic_negativo"
            android:drawableRight="@drawable/ic_star"
            android:inputType="numberPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/historico3" />

        <EditText
            android:id="@+id/historico5"
            style="@style/Edit_Text"
            android:layout_width="323dp"
            android:layout_height="68dp"
            android:layout_marginTop="16dp"
            android:drawableLeft="@drawable/ic_positivo"
            android:drawableRight="@mipmap/ic_star"
            android:inputType="numberPassword"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/historico4" />


        <EditText
            android:id="@+id/profissonais"
            android:layout_width="158dp"
            android:layout_height="48dp"
            android:layout_marginTop="20dp"
            android:drawableRight="@drawable/ic_service"
            android:ems="10"
            android:inputType="textPersonName"
            android:text="Profissonais"
            android:textSize="21dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/containerComponents" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.core.widget.NestedScrollView>
